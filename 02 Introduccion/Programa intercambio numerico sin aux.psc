Algoritmo IntercambioSinAUX
	Definir num1, num2 Como Real;
	Escribir Sin Saltar "Ingrese el primer numero ";
	Leer num1;
	Escribir sin saltar "Ingrese el segundo numero ";
	Leer num2;
	num1=num1+num2
	num2=num1-num2
	num1=num1-num2
	Escribir "El primer numero es ", num1;
	Escribir "El segundo numero es ", num2;
	
FinAlgoritmo
