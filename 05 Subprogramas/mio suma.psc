
Algoritmo ej_1_cambio_variables
	Definir v,v1, res Como entero
	Escribir "Ingrese dos variables para intercambiar su suma"
	Leer v,v1
	
	res=sum_num(v,v1)
	Escribir "suma da " res
	
FinAlgoritmo

Funcion suma <- sum_num(v,v1)
	definir suma Como Entero
	suma=(v+v1)*2
	
	
Fin Funcion

